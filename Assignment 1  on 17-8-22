#include<stdio.h>
int main()
{
	int a[50],i,n,tol=0,miss=0,sum=0;
	printf("Enter size of an array");
	scanf("%d",&n);
	printf("Enter consequitive Elements");
	for(i=0;i<n-1;i++)
	{
	scanf("%d",&a[i]);
	sum=sum+a[i];
    }
    tol=n*(n+1)/2;
    miss=tol-sum;
    printf("%d",miss);
    return 0;
    
}
